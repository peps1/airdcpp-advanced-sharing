    "npm-check-updates": "^9.0.4",
    "nyc": "^15.1.0",
    "prettyjson": "^1.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "airdcpp-apisocket": "^2.4.0-beta.3",
