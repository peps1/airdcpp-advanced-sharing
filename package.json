  "homepage": "https://github.com/peps1/airdcpp-advanced-sharing",
  "license": "MIT",
  "author": {
    "name": "pepsi"
  },
  "keywords": [
    "airdcpp",
    "airdcpp-extensions",
    "airdcpp-extensions-public"
